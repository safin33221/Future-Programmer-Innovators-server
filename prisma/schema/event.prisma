model Event {
    id          String      @id @default(uuid())
    title       String
    description String?
    eventType   String
    startTime   DateTime
    endTime     DateTime
    location    String?
    status      EventStatus @default(DRAFT)
    createdBy   String

    // Relations
    // user User @relation(fields: [createdBy], references: [id], onDelete: Cascade)

    // Timestamps
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("events")
}
