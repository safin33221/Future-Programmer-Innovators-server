{
  "compilerOptions": {
    "rootDirs": [
      "src",
      "generated"
    ],
    "outDir": "dist",
    "module": "commonjs",
    "target": "es2016",
    "lib": [
      "esnext"
    ],
    "types": [
      "node"
    ],
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "strict": false,
    "exactOptionalPropertyTypes": false,
    "noUncheckedIndexedAccess": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*",
    "generated/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}